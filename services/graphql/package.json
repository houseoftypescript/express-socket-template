{
  "name": "@express/graphql",
  "version": "0.0.1",
  "description": "Express.js Template - GraphQL",
  "main": "dist/server.js",
  "repository": "git@github.com:houseoftypescript/graphql.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 16",
    "npm": ">= 8",
    "yarn": ">= 1"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "start": "node .",
    "start:dev": "kill-port 5000 && nodemon",
    "start:prod": "node .",
    "prepare": "husky install || true",
    "lint": "eslint . --fix --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@apollo/server": "4.6.0",
    "@graphql-tools/merge": "8.4.0",
    "axios": "1.3.5",
    "cors": "2.8.5",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-depth-limit": "1.1.0",
    "http-errors": "2.0.0",
    "pino": "8.11.0",
    "pino-pretty": "10.0.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/webpack-node-externals": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "eslint": "8.37.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.22",
    "prettier": "2.8.7",
    "pretty-quick": "3.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "5.0.3",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  }
}
