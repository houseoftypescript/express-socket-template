{
  "name": "express-template",
  "version": "0.0.1",
  "description": "Express Template",
  "main": "dist/server.js",
  "repository": "git@github.com:houseoftypescript/express-template.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 16",
    "npm": ">= 8",
    "yarn": ">= 1"
  },
  "scripts": {
    "prebuild": "rimraf dist && yarn docs:swagger:html",
    "build": "webpack --mode=production",
    "start": "node .",
    "start:dev": "kill-port 8080 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "start:prod": "node .",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger/swagger.json > ./docs/swagger/swagger.yaml",
    "docs:swagger:html": "redoc-cli build ./docs/swagger/swagger.json --output ./public/index.html --title \"Express APIs\"",
    "lint": "eslint . --fix --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.2",
    "helmet": "6.0.1",
    "http-errors": "2.0.0",
    "morgan": "1.10.0",
    "pino": "8.11.0",
    "pino-pretty": "10.0.0",
    "tsoa": "5.1.1"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.5.0",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.11",
    "@types/webpack-node-externals": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "@webpack-cli/generators": "3.0.1",
    "concurrently": "8.0.1",
    "eslint": "8.37.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.22",
    "openapi-to-postmanv2": "4.10.2",
    "prettier": "2.8.7",
    "pretty-quick": "3.1.3",
    "prisma": "4.12.0",
    "redoc-cli": "0.13.21",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "5.0.2",
    "webpack": "5.77.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  }
}
