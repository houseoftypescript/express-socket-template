{
  "name": "express-template",
  "version": "0.0.1",
  "description": "Express.js Template",
  "main": "index.js",
  "repository": "git@github.com:houseoftypescript/express-template.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GNU GPL 3.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "build:apis": "yarn workspace @express/apis build",
    "dev:apis": "yarn workspace @express/apis start:dev",
    "lint:apis": "yarn workspace @express/apis lint",
    "build:graphql": "yarn workspace @express/graphql build",
    "dev:graphql": "yarn workspace @express/graphql start:dev",
    "lint:graphql": "yarn workspace @express/graphql lint"
  },
  "devDependencies": {
    "husky": "8.0.3",
    "prettier": "2.8.7",
    "pretty-quick": "3.1.3"
  },
  "workspaces": [
    "services/*"
  ]
}
